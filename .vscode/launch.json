{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug GitHub Action",
        "skipFiles": [ "<node_internals>/**" ],
        "runtimeExecutable": "ts-node",
        "args": [
          "${workspaceFolder}/src/main.ts"
        ],
        "cwd": "${workspaceFolder}/_repo_",
        "console": "internalConsole",
        "env": {
          "GITHUB_EVENT_PATH": "${workspaceFolder}/.vscode/mock-event.json",
          "GITHUB_WORKSPACE": "${workspaceFolder}/_repo_",
          "GITHUB_REPOSITORY": "dorin7bogdan/ufto-tests", //"mbtuser/mbtuser"
          "GITHUB_ACTION_REPOSITORY": "dorin7bogdan/github-action-ft-integration",
          "GITHUB_ACTION_REF": "main",
          "GITHUB_EVENT_NAME": "workflow_dispatch", // Simulate manual trigger
          "INPUT_OCTANEURL": "https://integration-master-dev.almoctane.com", //"https://qa8.almoctane.com",
          "INPUT_OCTANESHAREDSPACE": "1004",
          "INPUT_OCTANEWORKSPACE": "2002",
          "INPUT_OCTANECLIENTID": "***",
          "INPUT_OCTANECLIENTSECRET": "***",
          "INPUT_GITHUBTOKEN": "***",
          "INPUT_LOGLEVEL": "2",
          "INPUT_TESTINGTOOLTYPE": "MBT",
          "INPUT_MINSYNCINTERVAL": "0.25"
        }
      }
    ]
}